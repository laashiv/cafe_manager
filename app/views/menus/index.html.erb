    <section id = "sideMenu">
        <div class =  "toggle-btn" onclick = "toggleSidebar()">
            <span></span>
            <span></span>
        </div>
        <nav id = "dashboard-menu">
            <a id = "items" href = "#" class = "active-dashboard">
                <i class = "fas fa-home" aria-hidden = "true"></i>
            MENU</a>
            <a id = "items" href = "/active-menu">
                <i class = "fab fa-meetup" aria-hidden = "true"></i>
            ACTIVE-MENU</a>
            <a id = "items" href = "/menu_items">
                <i class = "fas fa-utensils" aria-hidden = "true"></i>
            MENU_ITEMS</a>
            <a id = "items" href = "/orders">
                <i class = "fas fa-history" aria-hidden = "true"></i>
            ORDERS</a>
            <a id = "items" href = "/users-list">
                <i class = "fas fa-users" aria-hidden = "true"></i>
            USERS</a>
            <a id = "items" href = "/clerks-list">
                <i class = "fas fa-address-card" aria-hidden = "true"></i>
            CLERKS</a>
            <a id = "items" href = "/sales">
                <i class = "fas fa-rupee-sign" aria-hidden = "true"></i>
            SALES</a>
             <a id = "items" href = "/cdash">
                <i class = "fas fa-cookie-bite" aria-hidden = "true"></i>
            OFFLINE ORDER</a>
        </nav>
    </section>

    <header class = "navigationbar">
        <diV class = "dashboard-text">DASHBOARD</div>
        <div class = "logo">
            <img class="navbar-brand" src="<%= asset_path('logo.png') %>" />
        </div>
        <p class = "restaurantname">CHEFOODU</p>

              <a
                href="#"
                class="nav-link dropdown-toggle"
                id="logout"
                data-toggle="dropdown"
                aria-haspopup="true"
                aria-expanded="false"
              >
                <i id = "loggedin-icon" class = "fas fa-user-circle" aria-hidden = "true"></i>
              </a>
              <div
                class="dropdown-menu dropdown-menu-right"
                aria-labelledby="logout"
              >
                <p class="logout-link"><%= link_to "Sign-out", destroy_session_path, method: "delete" %></p>
              </div>
    </header>



    <section class = "New-Menu">
        <%= form_with(url: "/menus", method: "POST", class: "Add-Menu") do %>
            <%= text_field_tag("name", nil, class: "Menu-Name", placeholder: "Wanna Add a New Menu?") %>
            <%= submit_tag("Add", class: "Addmenu-Button") %>
        <% end %>
    </section>



    <section>
    <%= render partial: "Menus-List", locals: { menus: Menu.all , menu_items: MenuItem.all, links: LinkMenuItem.all} %>
    </section>

    <script>
        function toggleSidebar(){
        document.getElementById("sideMenu").classList.toggle('active');
        }
    </script>
