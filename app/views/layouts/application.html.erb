    <% end %>-->

    <%= yield %>

    <%= javascript_pack_tag 'users', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'cdash', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'cart_items', 'data-turbolinks-track': 'reload' %>
  </body>
